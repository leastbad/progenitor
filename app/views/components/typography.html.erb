<%= render "components/typography/controls" %>
<div class="row">
  <div class="col-12 mb-4">
    <div class="card border-light shadow-sm components-section">
      <div class="card-body">
        <div class="row mb-4 mb-lg-5">
          <div class="col-12 col-md-6">
            <h2 class="h5 mb-3">Headings</h2>
            <h1>h1. Heading</h1>
            <h2>h2. Heading</h2>
            <h3>h3. Heading</h3>
            <h4>h4. Heading</h4>
            <h5>h5. Heading</h5>
            <h6>h6. Heading</h6>
          </div>
          <div class="col-12 col-md-6">
            <h2 class="h5 mb-3">Display Headings</h2>
            <h1 class="display-1">Display 1</h1>
            <h1 class="display-2">Display 2</h1>
            <h1 class="display-3">Display 3</h1>
            <h1 class="display-4">Display 4</h1>
          </div>
        </div>
        <div class="row">
          <div class="col-md-4">
            <div class="mb-5">
              <h2 class="h5 mb-3">Paragraphs</h2>
            </div>
          </div>
        </div>
        <div class="row mb-4 mb-lg-5">
          <div class="col-md-6">
            <div class="mb-3">
              <span class="h6 fw-bold">Simple paragraph</span>
            </div>
            <p><%= Faker::TvShows::BojackHorseman.quote %>.</p>
          </div>
          <div class="col-md-6">
            <div class="mt-5 mb-3 mt-sm-0">
              <span class="h6 fw-bold">Lead paragraph</span>
            </div>
            <p class="lead"><%= Faker::TvShows::BojackHorseman.quote %>.</p>
          </div>
        </div>
        <div class="row mt-4 mb-3 align-items-center">
          <div class="col-sm-2">
            <small class="text-uppercase text-muted">Dark text</small>
          </div>
          <div class="col-sm-10">
            <p class="text-dark mb-0">
              <%= Faker::TvShows::BojackHorseman.quote %>.
            </p>
          </div>
        </div>
        <div class="row py-3 align-items-center">
          <div class="col-sm-2">
            <small class="text-uppercase text-muted">Primary text</small>
          </div>
          <div class="col-sm-10">
            <p class="text-primary mb-0">
              <%= Faker::TvShows::BojackHorseman.quote %>.
            </p>
          </div>
        </div>
        <div class="row py-3 align-items-center">
          <div class="col-sm-2">
            <small class="text-uppercase text-muted">Secondary text</small>
          </div>
          <div class="col-sm-10">
            <p class="text-secondary mb-0">
              <%= Faker::TvShows::BojackHorseman.quote %>.
            </p>
          </div>
        </div>
        <div class="row py-3 align-items-center">
          <div class="col-sm-2">
            <small class="text-uppercase text-muted">Tertiary text</small>
          </div>
          <div class="col-sm-10">
            <p class="text-tertiary mb-0">
              <%= Faker::TvShows::BojackHorseman.quote %>.
            </p>
          </div>
        </div>
        <div class="row py-3 align-items-center">
          <div class="col-sm-2">
            <small class="text-uppercase text-muted">Info text</small>
          </div>
          <div class="col-sm-10">
            <p class="text-info mb-0">
              <%= Faker::TvShows::BojackHorseman.quote %>.
            </p>
          </div>
        </div>
        <div class="row py-3 align-items-center">
          <div class="col-sm-2">
            <small class="text-uppercase text-muted">Danger text</small>
          </div>
          <div class="col-sm-10">
            <p class="text-danger mb-0">
              <%= Faker::TvShows::BojackHorseman.quote %>.
            </p>
          </div>
        </div>
        <div class="row py-3 align-items-center">
          <div class="col-sm-2">
            <small class="text-uppercase text-muted">Success text</small>
          </div>
          <div class="col-sm-10">
            <p class="text-success mb-0">
              <%= Faker::TvShows::BojackHorseman.quote %>.
            </p>
          </div>
        </div>
        <div class="row">
          <div class="col-md-4">
            <div class="mt-6 mb-5">
              <span class="h6">Blockquote</span>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-md-8">
            <blockquote class="blockquote text-center">
              <%= Faker::TvShows::Seinfeld.quote %>
              <footer class="blockquote-footer mt-3 text-primary">
                <%= Faker::TvShows::Seinfeld.character %>
              </footer>
            </blockquote>
          </div>
        </div>
        <div class="row">
          <div class="col-md-4">
            <div class="mt-6 mb-5">
              <span class="h6">Lists</span>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-md-6">
            <div class="mb-3">
              <span class="h6">Unordered</span>
            </div>
            <ul>
              <li><%= Faker::TvShows::TwinPeaks.quote %></li>
              <li><%= Faker::TvShows::TwinPeaks.quote %></li>
              <li>
                <%= Faker::TvShows::TwinPeaks.quote %>
                <ul>
                  <li><%= Faker::TvShows::TwinPeaks.quote %></li>
                  <li><%= Faker::TvShows::TwinPeaks.quote %></li>
                  <li><%= Faker::TvShows::TwinPeaks.quote %></li>
                </ul>
              </li>
              <li><%= Faker::TvShows::TwinPeaks.quote %></li>
            </ul>
          </div>
          <div class="col-md-6">
            <div class="mt-5 mb-3 mt-sm-0">
              <span class="h6 fw-bold">Ordered</span>
            </div>
            <ol>
              <li><%= Faker::TvShows::TwinPeaks.quote %></li>
              <li><%= Faker::TvShows::TwinPeaks.quote %></li>
              <li>
                <%= Faker::TvShows::TwinPeaks.quote %>
                <ol>
                  <li><%= Faker::TvShows::TwinPeaks.quote %></li>
                  <li><%= Faker::TvShows::TwinPeaks.quote %></li>
                  <li><%= Faker::TvShows::TwinPeaks.quote %></li>
                </ol>
              </li>
              <li><%= Faker::TvShows::TwinPeaks.quote %></li>
            </ol>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
